<h3>Song Form</h3>
<%= form_for(@song) do |f| %>
    <label>Song name: </label>
    <%= f.text_field :name %>

    <br><br>

    <label>Artist id: </label>
    <%= f.text_field :artist_id %>

    <br><br>

    <label>Genre id: </label>
    <%= f.text_field :genre_id %>

    <%= f.submit %>
<% end %>